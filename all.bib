	Journal = {Communications in Mathematical Physics},
	Month = {Jul},
	Number = {1},
	Pages = {413--468},
	Title = {Kitaev Lattice Models as a {H}opf Algebra Gauge Theory},
	Url = {https://doi.org/10.1007/s00220-017-2860-7},
	Volume = {353},
	Year = {2017},
