	Eprint = {arXiv:1002.2540},
	Keywords = {categorical quantum mechanics, entanglement, graphical calculi, category theory},
	Pages = {297-308},
	Title = {The Compositional Structure of Multipartite Quantum Entanglement},
	Volume = {6199},
	Year = {2010},
