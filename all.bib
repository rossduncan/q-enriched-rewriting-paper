	Journal = {Foundations of Physics},
	Keywords = {Quantum foundations; Spekkens' toy theory; Stabilizer quantum mechanics; Graphical calculus; Categorical quantum mechanics; Graph states; $$\psi $$  -epistemic theory},
	Language = {English},
	Pages = {1-34},
	Publisher = {Springer US},
	Title = {A Complete Graphical Calculus for {S}pekkens' Toy Bit Theory},
	Url = {http://dx.doi.org/10.1007/s10701-015-9957-7},
